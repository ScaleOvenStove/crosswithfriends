{
  "name": "@crosswithfriends/frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@crosswithfriends/shared": "workspace:*",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/material": "^7.3.5",
    "@tanstack/react-query": "^5.90.10",
    "@tanstack/react-virtual": "^3.0.0",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "@types/react-router-dom": "^5.3.3",
    "canvas-confetti": "^1.9.4",
    "classnames": "^2.5.1",
    "cross-env": "^10.1.0",
    "firebase": "^12.6.0",
    "gaussian": "^1.3.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.9.6",
    "socket.io-client": "^4.8.1",
    "sweetalert2": "^11.26.3",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@firebase/rules-unit-testing": "^5.0.0",
    "@playwright/experimental-ct-react": "^1.56.1",
    "@playwright/test": "^1.56.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/canvas-confetti": "^1.9.0",
    "@types/node": "^20.19.25",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "jsdom": "^27.2.0",
    "prettier": "^3.6.2",
    "rollup-plugin-visualizer": "^6.0.5",
    "typescript": "^5.9.3",
    "vite": "^7.2.2",
    "vitest": "^4.0.10"
  },
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "dev:local": "cross-env VITE_USE_LOCAL_SERVER=1 vite",
    "dev:prod": "cross-env VITE_ENV=production VITE_USE_LOCAL_SERVER=1 vite",
    "build": "vite build",
    "build:prod": "cross-env NODE_ENV=production vite build",
    "preview": "vite preview",
    "run:prod": "vite preview",
    "test": "vitest",
    "test:firebase:rules": "vitest run src/tests/firebase/rules",
    "test:firebase:rules:watch": "vitest watch src/tests/firebase/rules",
    "test:component": "playwright test -c playwright-ct.config.ts",
    "test:component:ui": "playwright test -c playwright-ct.config.ts --ui",
    "test:component:debug": "playwright test -c playwright-ct.config.ts --debug",
    "test:e2e": "playwright test -c playwright.config.ts",
    "test:e2e:ui": "playwright test -c playwright.config.ts --ui",
    "test:e2e:debug": "playwright test -c playwright.config.ts --debug",
    "test:all": "playwright test -c playwright-ct.config.ts && playwright test -c playwright.config.ts",
    "test:install": "playwright install",
    "firebase:emulators": "firebase emulators:start",
    "firebase:emulators:exec": "firebase emulators:exec --only database",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\""
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "sideEffects": false
}
